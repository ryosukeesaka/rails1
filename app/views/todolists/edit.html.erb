<h1>Editing Book</h1>
<%= form_for(@list,url:"/todolists/#{@list.id}") do |f| %>
	<h4>Title</h4>
	<%= f.text_field :title %>
	<h4>Body</h4>
	<%= f.text_area :body %><br>
	<%= f.submit "Update Back" %><br>
	<%= link_to "show", todolist_path(@list.id) %>
	|
	<%= link_to "Back", todolists_path(@list.id) %>
	<% end %>